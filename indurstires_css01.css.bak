.icon-container {
    display: flex;
    justify-content: flex-start; /* Change to flex-start to allow manual spacing */
    margin-top: 5px; /* Adjust the spacing between icons */
    margin-bottom: 0; /* Adjust the space below the icons */
    margin-right:130px;
}
 
.icon {
    display: inline-block;
    position: relative;
    margin-right: 10px; /* Adjust the right margin to control spacing between icons */
}

.icon:last-child {
    margin-right: 0; /* Remove margin for the last icon to prevent extra spacing */
}

.icon img {
    width: 15px; /* Adjust the size of your icon */
    height: 15px; /* Adjust the size of your icon */
    transition: filter 0.3s ease-in-out;
}

.icon:hover img {
    filter: brightness(5%) sepia(100%) hue-rotate(30deg); /* Adjust the filter values for the desired effect */
}

.icon:hover::before {
    content: "";
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 30px; /* Adjust the size of the circle */
    height: 30px; /* Adjust the size of the circle */
    background-color: yellow;
    border-radius: 50%;
}

