                 <img class="slider-image12" src="MicrosoftTeams-image (49).png" alt="Image 2">
                 <img class="slider-image12" src="MicrosoftTeams-image (51).png" alt="Image 3">
                 <img class="slider-image12" src="MicrosoftTeams-image (52).png" alt="Image 4">
                 <img class="slider-image12" src="MicrosoftTeams-image (53).png" alt="Image 5">
                 <img class="slider-image12" src="MicrosoftTeams-image (54).png" alt="Image 6">
	         
             </div>
             <div class="slider-navigation12">
                 <button onclick="prevSlide()">&#8249;</button>
                 <button onclick="nextSlide()">&#8250;</button>
             </div>
        </div>

        <script>
            const slider = document.getElementById('image-slider12');
            let count = 0;
        
            function nextSlide() {
                count = (count + 1) % 6; // Adjust the modulus value based on the number of unique images
                updateSlider();
            }
        
            function prevSlide() {
                count = (count - 1 + 6) % 6; // Ensure count is non-negative
                updateSlider();
            }
        
            function updateSlider() {
                const translateValue = -count * (100 / 3);
                slider.style.transform = `translateX(${translateValue}%)`;
            }
        
            // Change slide every 3 seconds (adjust the interval as needed)
            setInterval(nextSlide, 3000);
        </script>
		    
		    
		    
		    
		    
		     
		    
